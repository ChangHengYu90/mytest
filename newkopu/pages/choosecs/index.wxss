@import "../../teml/cascade.wxss";
.dqdw{
  width: 100%;
  padding: 0 35rpx;
  box-sizing: border-box;
}
.dqdw-t{
  width: 100%;
  height: 90rpx;
  display: flex;
  align-items: center;
  font-size: 34rpx;
  color: #999;
}
.dwcs{
  width: 100%;
  height: 120rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.dwcs-l{
 padding: 0 25rpx;
  height: 70rpx;
  background-color: rgb(30, 99, 226);
  border-radius: 15rpx;
  display: flex;
  align-items: center;
  color: #fff;
  font-size: 26rpx;
}
.dwcs-l image{
 height: 30rpx;
 width: 30rpx;
 margin-right: 15rpx;
}
.dwcs-r image{
 height: 30rpx;
 width: 30rpx;
 margin-right: 15rpx;
}
.dwcs-r{
  padding: 0 25rpx;
  height: 70rpx;
  color: rgb(30, 99, 226);
  display: flex;
  align-items: center;
  font-size: 26rpx;
}
.cslist{
  width: 100%;
  height: 70rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1rpx solid #ddd;
  font-size: 30rpx;
  color: #444

}
.cslist image{
  width: 15rpx;
  height: 25rpx;
  
}